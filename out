    .scope S_0x7fe89b004260;
T_3 ;
    %wait E_0x7fe89b0049b0;
    %load/vec4 v0x7fe89b014cd0_0;
    %addi 1, 0, 8;
    %assign/vec4 v0x7fe89b014cd0_0, 0;
    %jmp T_3;
    .thread T_3;
